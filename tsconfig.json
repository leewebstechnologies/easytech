{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/service/item/ServiceItem.jsx", "src/components/CaseStudy/CaseStudy.jsx", "src/components/header/menu/Menu.jsx", "src/components/header/topnav/Topnav.jsx", "src/components/PaymentGateway/PaymentGateway.jsx", "src/components/PaymentGateway/PaymentTwo.jsx", "src/components/service/Service.jsx", "src/components/slider/Slider.jsx", "src/app/about/page.jsx", "src/app/blog/page.jsx", "src/app/case/page.jsx", "src/app/contact/page.jsx", "src/app/services/page.jsx", "src/app/layout.jsx", "src/app/page.jsx"],
  "exclude": ["node_modules"]
}
